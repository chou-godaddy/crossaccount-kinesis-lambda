{
    "main": [
      "internal/cmd/crossaccount-kinesis-lambda/main.go"
    ],
    "release": {
      "type": "git",
      "app-name": "xaccount-kin-",
      "team_prefix": "rg0",
      "project_code": "rg0",
      "tagging": {
        "prod": {
          "increment": "MINOR",
          "release": true
        }
      },
      "builds": [
        {
          "type": "awslambda",
          "name": "crossaccount-kinesis-lambda",
          "cgoEnabled": "1",
          "entryPoint": "internal/cmd/crossaccount-kinesis-lambda/main.go",
          "files": {
            "internal/config/config.json": "internal/config/config.json"
          },
          "steps": [
            {
              "type": "aws-deployment",
              "regions": [
                "us-west-2"
              ]
            },
            {
              "type": "standard-lambda-deployment",
              "regions": [
                "us-west-2"
              ],
              "timeout": "180",
              "hosted_zone": "fulfillment.domains.{env_dns}.aws.gdcld.net",
              "lambda_custom_role": "fulfillment-kinesis-consumer-role",
              "skip_tests": true
            }
          ]
        }
      ]
    }
  }
  